{
  "name": "kubevirt-plugin",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "clean": "rm -rf dist",
    "build": "NODE_ENV=production NODE_OPTIONS=--max-old-space-size=8192 rm -rf dist && yarn ts-node node_modules/.bin/webpack",
    "dev": "NODE_ENV=development NODE_OPTIONS=--max-old-space-size=8192 rm -rf dist && yarn ts-node ./node_modules/.bin/webpack serve --progress",
    "lint": "eslint src cypress --color",
    "lint:fix": "yarn lint --fix",
    "check-types": "tsc",
    "i18n": "i18next \"src/**/*.{js,jsx,ts,tsx}\" -c i18next-parser.config.js",
    "ts-node": "ts-node -O '{\"module\":\"commonjs\"}'",
    "postinstall": "cp node_modules/@openshift-console/dynamic-plugin-sdk/doc/console-extensions.md .",
    "test": "jest",
    "test-cov": "jest --coverage",
    "cypress-merge": "mochawesome-merge ./gui-test-screenshots/cypress_report*.json > ./gui-test-screenshots/cypress.json",
    "cypress-generate": "marge -o ./gui-test-screenshots/ -f cypress-report -t 'Kubevirt Cypress Test Results' -p 'Kubevirt Test Results' --showPassed false --assetsDir ./gui-test-screenshots/cypress/assets ./gui-test-screenshots/cypress.json",
    "cypress-postreport": "yarn cypress-merge && yarn cypress-generate",
    "test-cypress": "node_modules/.bin/cypress open --config-file ./cypress/cypress.json --env openshift=true",
    "test-cypress-docker": "echo 'visit http://localhost:10000' && DISPLAY=:14 LIBGL_ALWAYS_INDIRECT=0 $(npm bin)/cypress open --config-file ./cypress/cypress.json --env openshift=true",
    "test-cypress-headless": "node --max-old-space-size=4096 $(npm bin)/cypress run --config-file ./cypress/cypress.json --env openshift=true --headless",
    "start-console": "./start-console.sh"
  },
  "consolePlugin": {
    "name": "kubevirt-plugin",
    "version": "0.0.0",
    "displayName": "Kubevirt Plugin",
    "exposedModules": {
      "VirtualMachinesList": "./views/virtualmachines/list/VirtualMachinesList.tsx",
      "TemplatesCatalog": "./views/catalog/Catalog.tsx",
      "VirtualMachineNavPage": "./views/virtualmachines/details/VirtualMachineNavPage.tsx",
      "VirtualMachinesInstancePage": "./views/virtualmachinesinstance/details/VirtualMachinesInstancePage.tsx",
      "useVirtualMachineActionsProvider": "./views/virtualmachines/actions/hooks/useVirtualMachineActionsProvider.ts",
      "VirtualMachinesInstancesList": "./views/virtualmachinesinstance/list/VirtualMachinesInstancesList.tsx",
      "useVirtualMachineInstanceActionsProvider": "./views/virtualmachinesinstance/actions/hooks/useVirtualMachineInstanceActionsProvider.tsx",
      "ClusterOverviewPage": "./views/clusteroverview/overview/components/ClusterOverviewPage.tsx"
    },
    "dependencies": {
      "@console/pluginAPI": "*"
    },
    "disableStaticPlugins": [
      "@console/kubevirt-plugin"
    ]
  },
  "dependencies": {
    "@kubevirt-ui/components": "^0.0.10",
    "@patternfly/quickstarts": "2.0.1",
    "@patternfly/react-catalog-view-extension": "^4.43.15",
    "@patternfly/react-code-editor": "^4.39.19",
    "@patternfly/react-core": "4.192.15",
    "@patternfly/react-table": "^4.67.5",
    "@types/byte-size": "^8.1.0",
    "byte-size": "^8.1.0",
    "date-fns": "^2.28.0",
    "fast-xml-parser": "^4.0.7",
    "global": "^4.4.0",
    "immer": "^9.0.12",
    "js-yaml": "^4.1.0",
    "mochawesome-report-generator": "^6.0.1",
    "monaco-editor": "^0.33.0",
    "monaco-editor-webpack-plugin": "^7.0.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-helmet": "^6.1.0",
    "react-i18next": "^11.14.3",
    "react-monaco-editor": "^0.47.0",
    "react-router-dom": "5.x",
    "react-tagsinput": "^3.19.0",
    "unique-names-generator": "^4.6.0",
    "use-immer": "^0.6.0",
    "validator": "^13.7.0"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.11.0",
    "@kubevirt-ui/kubevirt-api": "^0.0.37",
    "@openshift-console/dynamic-plugin-sdk": "0.0.6",
    "@openshift-console/dynamic-plugin-sdk-internal": "^0.0.5",
    "@openshift-console/dynamic-plugin-sdk-webpack": "0.0.6",
    "@testing-library/jest-dom": "^5.16.1",
    "@testing-library/react": "^12.1.2",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.0.3",
    "@types/js-yaml": "^4.0.5",
    "@types/node": "^16.11.12",
    "@types/react": "^17.0.37",
    "@types/react-router-dom": "5.x",
    "@types/webpack-dev-server": "^4.5.0",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "copy-webpack-plugin": "^6.4.1",
    "css-loader": "6.6.0",
    "cypress": "8.5.x",
    "cypress-multi-reporters": "^1.5.0",
    "eslint": "^8.10.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-chai-friendly": "^0.7.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsdoc": "^37.9.7",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "i18next": "^21.6.0",
    "i18next-parser": "3.x",
    "jest": "^27.4.5",
    "mocha": "^9.2.0",
    "mocha-junit-reporter": "^2.0.2",
    "mochawesome": "^7.0.1",
    "mochawesome-merge": "^4.2.1",
    "prettier": "^2.5.1",
    "resolve-url-loader": "^5.0.0",
    "sass": "^1.49.7",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.2.1",
    "ts-jest": "^27.1.1",
    "ts-loader": "9.x",
    "ts-node": "10.x",
    "tsconfig-paths-webpack-plugin": "^3.5.1",
    "typescript": "^4.5.5",
    "webpack": "^5.68.0",
    "webpack-cli": "4.9.x",
    "webpack-dev-server": "^4.7.4"
  },
  "resolutions": {
    "webpack": "^5.68.0"
  },
  "packageManager": "yarn@3.2.0"
}
